<!doctype html>
<html lang="{{locale}}">
<head>
  {{#each locales}}
    {{#ifnotlocale this}}
      <link rel="alternate" hreflang="{{this}}" href="{{{langlink this}}}" />
    {{/ifnotlocale}}
  {{/each}}
  
  <meta charset="UTF-8" />
  <title>{{title}}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="{{assetsPath}}{{ fingerprint.[styles/styles.css] }}" rel="stylesheet" type="text/css" />
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', '{{gaTrackerId}}', 'auto');
    ga('send', 'pageview');

  </script>

  <meta property="og:title" content="{{title}}" />
  <meta property="og:url" content="http://www.ravintolavinkkeli.fi" />
  <meta property="og:description" content="{{description}}" />
  
  <meta name="description" content="{{description}}" />
  <meta name="rating" content="Safe For Kids" />
  <meta name="author" content="Ravintola Vinkkeli Helsinki Oy" />
  <meta name="copyright" content="(C) Ravintola Vinkkeli Helsinki Oy" />
  <meta name="robots" content="index,follow" />
</head>
<body>
  {{> navigation }}
  <div class="main-wrapper">
      {{{ contents }}}
  </div>

  {{> footer }}
  {{> scripts }}
</body>
</html>